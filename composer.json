{
    "name": "kilip/omed-backend",
    "description": "Anthonius Munthi Demo application",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0 || ^7.1 || ^7.2",
        "ext-apcu": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.0",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "friendsofsymfony/jsrouting-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.0",
        "behat/behat": "~3.4@dev",
        "behat/mink-browserkit-driver": "^1.3.3@dev",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.0",
        "coduo/php-matcher": "~3.0@dev",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "hautelook/alice-bundle": "^2.0",
        "leanphp/phpspec-code-coverage": "^4.0",
        "phpspec/phpspec": "^4.3",
        "phpunit/php-code-coverage": "^5.3",
        "phpunit/phpcov": "^4.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Omed\\": "src/backend",
            "Spec\\": "spec/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omed\\Test\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "fos:js-routing:dump --target=%PUBLIC_DIR%/js/routes.js": "symfony-cmd"
        },
        "compile": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C3EX6B6ZMWFPGA2PCFM9MX0G",
            "allow-contrib": false
        }
    }
}
